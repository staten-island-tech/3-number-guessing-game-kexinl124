import random
random_int = random.randint(1,10)
guess = 0 
guess_history=[]

while True:
    guess=int(input("Guess numbers from 1-10: "))
    
    if guess < random_int:
     print('Too small')
     guess_history.append(guess)
     print(f"This is your guess history{guess_history}")

    elif guess > random_int:
     print('Too big')
     guess_history.append(guess)
     print(f"This is your guess history{guess_history}")

    else: 
     print('Correct') 
    print(f"This is your guess history{guess_history}")

