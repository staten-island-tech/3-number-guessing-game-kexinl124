import random
random_int = random.randint(1,10)
guess = 0 
guess_history=[]

while guess!=random_int:
    guess=int(input("Guess numbers from 1-10: "))
    
    if guess < random_int:
     print('Too small')
     guess_history.append(guess)
     print(f"Incorrect, keep on guessing. This is your guess history{guess_history}")

    elif guess > random_int:
     print('Too big')
     guess_history.append(guess)
     print(f"Incorrect, keep on guessing. This is your guess history{guess_history}")

    else: 
     print('Correct') 
    for i in range (1):
      print (f"This is your guess history{guess_history}")

print("You win")

